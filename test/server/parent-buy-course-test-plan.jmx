            <stringProp name="script">import java.util.ArrayList;
import java.util.Random;
import java.util.Collections;

import org.mozilla.javascript.json.JsonParser;
import org.mozilla.javascript.Scriptable;
import org.mozilla.javascript.Context;
import org.mozilla.javascript.NativeArray;

String dataStr = new String(data);
// System.out.println(dataStr);

Context cx = Context.enter();
Scriptable scope = cx.initStandardObjects();
Scriptable jsonObj = new JsonParser(cx, scope).parseValue(dataStr);
// System.out.println(jsonObj);

String subjectName = jsonObj.get(&quot;subject&quot;, scope);
System.out.println(subjectName);
vars.put(&quot;subjectName&quot;, subjectName);

NativeArray pricesDay = (NativeArray) jsonObj.get(&quot;prices&quot;, scope);
ArrayList gidList = new ArrayList(20);

for (int i = 0; i &lt; pricesDay.size(); i++) {
	Scriptable item = (Scriptable) pricesDay.get(i);
	Scriptable grade = (Scriptable) item.get(&quot;grade&quot;, scope);
	Integer id = (Integer) grade.get(&quot;id&quot;, scope);
	gidList.add(id);
}

// System.out.println(gidList);
int randInt = new Random().nextInt(gidList.size());
vars.put(&quot;grade_id&quot;, String.valueOf(gidList.get(randInt)));
System.out.println(&quot;grade_id: &quot;+vars.get(&quot;grade_id&quot;));
</stringProp>
