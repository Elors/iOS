            <stringProp name="script">import java.util.ArrayList;
import java.util.Random;

String dataStr = new String(data);
// System.out.println(dataStr);
final String ID_TAG = &quot;{\&quot;id\&quot;:&quot;;
ArrayList tidList = new ArrayList(20);
int p = 0, p2 = 0;
while (p&gt;=0 &amp;&amp; p2&gt;=0){
	p = dataStr.indexOf(ID_TAG, p2);
	if (p&gt;=0) {
		p2 = dataStr.indexOf(&apos;,&apos;, p+1);
		if (p2&gt;0) {
			tidList.add(dataStr.substring(p+ID_TAG.length(),p2));
		}
	}
}
// System.out.println(tidList);
int randInt = new Random().nextInt(tidList.size());
vars.put(&quot;teacher_id&quot;, tidList.get(randInt));
System.out.println(&quot;teacher_id: &quot;+vars.get(&quot;teacher_id&quot;));</stringProp>
