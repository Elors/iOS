<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="selectableItemBackgroundGeneral" format="color|reference" />
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="RubberIndicator">
        <attr name="smallCircleColor" format="color"/>
        <attr name="largeCircleColor" format="color"/>
        <attr name="outerCircleColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CircleView">
        <attr name="cv_color" format="color" />
        <attr name="cv_radius" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomTitleBar">
        <attr name="cusTitleText" format="string"></attr>
        <attr name="cusTitleTextSize" format="dimension"></attr>
        <attr name="cusTitleTextColor" format="color"></attr>
        <attr name="cusLeftBackgroud" format="reference|color"></attr>
        <attr name="cusRightTextColor" format="reference|color"></attr>
        <attr name="cusRightTextSize" format="dimension" ></attr>
        <attr name="cusRightBackgroud" format="reference|color"></attr>
        <attr name="cusRightText" format="string"></attr>
        <attr name="cusLeftShow" format="boolean" ></attr>
        <attr name="cusRightShow" format="boolean"></attr>
    </declare-styleable>
</resources>